{
  "name": "vsts-code-coverage-pullrequest-reporter",
  "version": "0.1.8",
  "description": "Visual Studio Team Services task that reports code coverage to pull request.",
  "main": "index.js",
  "author": "Igor Oleinikov",
  "license": "MIT",
  "scripts": {
    "test:watch": "jest --watch --config jest.json",
    "build": "tsc && copy.cmd && pack.cmd",
    "build:dev": "webpack --config ./webpack.config.js --env=dev",
    "build:prod": "webpack --config ./webpack.config.js --env=prod",
    "build:copy": "copy.cmd",
    "build:pack": "pack"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.51",
    "@types/q": "^1.0.6",
    "awesome-typescript-loader": "^3.3.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.2",
    "ts-jest": "^21.2.1",
    "typescript": "2.5.3",
    "webpack": "^3.8.1",
    "webpack-externals-plugin": "^1.0.0"
  },
  "dependencies": {
    "vso-node-api": "^6.2.8-preview",
    "vsts-task-lib": "^2.1.0"
  }
}
